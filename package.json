{
  "name": "adonis-ally",
  "version": "1.1.2",
  "description": "Social authenticator for adonis framework",
  "main": "index.js",
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "scripts": {
    "lint": "standard",
    "pretest": "npm run lint",
    "posttest": "npm run coverage",
    "test:local": "FORCE_COLOR=true node bin/index.js --local",
    "test": "nyc npm run test:local",
    "test:win": "set FORCE_COLOR=true && node bin/index.js --win",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "keywords": [
    "adonis",
    "adonisjs",
    "social-auth",
    "oauth"
  ],
  "author": "adonisjs,amanvirk",
  "license": "MIT",
  "devDependencies": {
    "@adonisjs/fold": "^4.0.2",
    "coveralls": "^2.11.12",
    "cz-conventional-changelog": "^1.2.0",
    "japa": "^1.0.4",
    "japa-cli": "^1.0.1",
    "nyc": "^11.1.0",
    "semver": "^5.4.1",
    "standard": "^8.6.0"
  },
  "standard": {
    "global": [
      "describe",
      "it",
      "use",
      "context",
      "before",
      "after",
      "beforeEach",
      "afterEach"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "cat-log": "^1.0.2",
    "got": "^6.3.0",
    "lodash": "^4.15.0",
    "node-exceptions": "^1.0.3",
    "oauth": "^0.9.14"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adonisjs/adonis-ally.git"
  },
  "bugs": {
    "url": "https://github.com/adonisjs/adonis-ally/issues"
  },
  "homepage": "https://github.com/adonisjs/adonis-ally#readme"
}
